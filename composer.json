{
  "name": "michaeldrennen/td-ameritrade-api",
  "description": "A PHP library used to interact with the TD Ameritrade API.",
  "type": "library",
  "require": {
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.4||^7.0",
    "symfony/dom-crawler": "^5.1",
    "symfony/css-selector": "^5.1",
    "chrome-php/chrome": "^0.8||^0.9",
    "nesbot/carbon": "^2.0",
    "michaeldrennen/calendar": "^1.0",
    "michaeldrennen/business-calendar": "^0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4||^9.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "michaeldrennen",
      "email": "michaeldrennen74@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "MichaelDrennen\\TDAmeritradeAPI\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MichaelDrennen\\TDAmeritradeAPI\\Tests\\": "tests"
    }
  }
}
